{
  "name": "task-mate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ENV_FILE=.env.local NODE_ENV=development node server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup:dynamodb": "node utils/create-tables-locally.js && node utils/seed-dynamodb.js",
    "up": "docker-compose up -d",
